{
  "name": "project-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run watch-translation & vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --coverage",
    "prettier:lint": "prettier . --check --ignore-unknown",
    "prettier:fix": "prettier . --write --list-different --ignore-unknown",
    "eslint:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "npm-run-all -c --parallel prettier:lint eslint:lint --silent",
    "lint:fix": "npm-run-all -c --sequential prettier:fix eslint:fix --silent",
    "extract-translation": "i18next 'src/**/*.{js,jsx,ts,tsx}' 'lib/**/*.{js,jsx,ts,tsx}' [-oc] --silent",
    "watch-translation": "npm-watch extract-translation"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.86",
    "@mui/material": "^5.8.4",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "i18next": "^21.8.10",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.32.1",
    "react-i18next": "^11.17.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-jest": "^28.1.3",
    "css-mediaquery": "^0.1.2",
    "eslint": "^8.21.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "i18next-parser": "^6.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "watch": {
    "extract-translation": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  }
}
