{
  "name": "project-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run watch-translation & vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "prettier:lint": "prettier . --check --ignore-unknown",
    "prettier:fix": "prettier . --write --list-different --ignore-unknown",
    "eslint:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "npm-run-all -c --parallel prettier:lint eslint:lint --silent",
    "lint:fix": "npm-run-all -c --sequential prettier:fix eslint:fix --silent",
    "extract-translation": "i18next 'src/**/*.{js,jsx,ts,tsx}' 'lib/**/*.{js,jsx,ts,tsx}' [-oc] --silent",
    "watch-translation": "npm-watch extract-translation"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.100",
    "@mui/material": "^5.10.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/react": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "axios": "^0.27.2",
    "history": "^5.3.0",
    "i18next": "^21.9.2",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-i18next": "^11.18.6",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-jest": "^29.0.3",
    "css-mediaquery": "^0.1.2",
    "eslint": "^8.23.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "i18next-parser": "^6.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^3.0.0-alpha",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
  "watch": {
    "extract-translation": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  }
}
