{
  "name": "project-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "i18next": "^21.6.16",
    "notistack": "^2.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.30.0",
    "react-i18next": "^11.16.7",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^5.20.0",
    "css-mediaquery": "^0.1.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "i18next-parser": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "yarn run watch-translation & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:lint": "prettier . --check --ignore-unknown",
    "prettier:fix": "prettier . --write --list-different --ignore-unknown",
    "eslint:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "npm-run-all -c --parallel prettier:lint eslint:lint --silent",
    "lint:fix": "npm-run-all -c --sequential prettier:fix eslint:fix --silent",
    "extract-translation": "i18next 'src/**/*.{js,jsx,ts,tsx}' 'lib/**/*.{js,jsx,ts,tsx}' [-oc] --silent",
    "watch-translation": "npm-watch extract-translation"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "extract-translation": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/tests/utils.tsx"
    ]
  }
}
