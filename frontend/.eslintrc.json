{
  "parser": "@typescript-eslint/parser",
  "extends": ["react-app", "react-app/jest"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}
